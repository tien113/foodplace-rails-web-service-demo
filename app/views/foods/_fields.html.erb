<%= render 'shared/error_messages', object: f.object %>

<%= f.text_field :food_name, { placeholder: "Name" } %>

<%= f.number_field :food_price, { placeholder: "Price" } %>

<%= f.url_field :food_image_url, { placeholder: "Image URL" } %>

<%= f.text_area :food_ingredient, { placeholder: "Ingredient", rows: 5 } %>

<%= collection_select(:food, :place_id, Place.all, :id, :place_name, { prompt: "Select Place" }, { class: 'span5' }) %>
